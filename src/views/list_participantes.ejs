<!DOCTYPE html>
<html lang="en">
<head>
	<title>Authentication with Node</title>
	<% include partials/head %>
</head>
<body>
	<div class="container">
		<div class="card text-center mt-5">
			<div class="card-body">
                <img width="100px" src="https://pbs.twimg.com/profile_images/836695399809302528/568l42NJ_400x400.jpg">
				<h1>
				<span class="fa fa-block"></span>Mis Participantes</h1>
				<table class="table"> 
                        <thead>
                          <tr>
                            <th>#</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% participantes.forEach(function(participante) { %> 
                          <tr>
                            <!-- Muestra solo instructores -->
                                <td><%= participante.access_code%></td>
                                <!-- Se envia por GET el id del instructor -->
                          </tr>
                          <% }); %>
                        </tbody>
                </table>
			</div>
		</div>
	</div>
</body>
</html>
