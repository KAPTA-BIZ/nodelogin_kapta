                                                                                            <td>
                                                                                                <%=option.clue%>
                                                                                            </td>
                                                                                            <td>
                                                                                                <%=option.match%>
                                                                                            </td>
                                                                                            <td>
                                                                                                <li class="<%=option.status%>" style="list-style-type: none">
                                                                                                    <div>
                                                                                                        <%=option.user_response%>
                                                                                                    </div>
                                                                                                </li>
                                                                                            </td>
                                                                                        </tr>
